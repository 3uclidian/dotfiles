! URxvt extensions
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.iso14755: False

URxvt.perl-ext-common: default,resize-font

*locale: en_US.UTF-8

!*.transparent: true
!*.shading: 20
!*.blurRadius: 5
*.font: -*-terminus-*-*-*-*-18-*-*-*-*-*-*-*, -*-unifont-*-*-*-*-*-*-*-*-*-*-*-*
!xft:UbuntuMono:size=14, 

*.scrollBar: false
*.letterSpace: 0
*.lineSpace: 2
*.internalBorder: 10
*.cursorBlink: true
URxvt.pointerBlank: true

