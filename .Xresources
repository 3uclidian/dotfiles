! Transparency
*.transparent:	true
*.shading:		60

! Font
*.font:		xft:UbuntuMono:size=14:antialias=true

! Get rid of scrollbar
*.scrollBar: 	false

! Letter spacing
*.letterSpace: 	0
*.lineSpace:	0

! Internal Border
*.internalBorder: 	30

! Blur
*.blurRadius:	5

! special
*.foreground:   #c8c8c8
*.background:   #1d1f21
*.cursorColor:  #c8c8c8

! black
*.color0:       #282a2e
*.color8:       #373b41

! red
*.color1:       #893030
*.color9:       #c14f4f

! green
*.color2:       #608431
*.color10:      #7aed6b

! yellow
*.color3:       #a1ad78
*.color11:      #acd78f

! blue
*.color4:       #58748d
*.color12:      #3c80ba

! magenta
*.color5:       #72567c
*.color13:      #a963be

! cyan
*.color6:       #5b837f
