# vim: filetype=sh
alias gs="git status"
alias gss="git status --short"

alias gd="git diff"
alias gdt="git difftool -y"

alias ga="git add"
alias gaa="git add -A"
alias gc="git commit"
alias gcm="git commit -m"

alias gp="git push"
alias gpu="git push -u"

alias gl="git log --graph --decorate --oneline"
alias gll="git log --graph --decorate"
